          <li class="nav-item"><%= link_to "About", about_path, class: "nav-link" %></li>
          <li class="nav-item"><%= link_to "Pricing",pricing_path, class: "nav-link" %></li>
          <li class="nav-item"><%= link_to "Contact",contact_path, class: "nav-link" %></li>
          <li class="nav-item pull-md-right"><%= link_to "Log in In",user_session_path, class: "nav-link" %></li>
          <li class="nav-item float-md-right"><%= link_to "Log out",destroy_user_session_path,  method: :delete, class: "nav-link" %></li>
          <li class="nav-item "><%= link_to "Sign Up", new_user_registration_path, class: "nav-link" %></li>
        
        </ul>
      </div>
    </nav>
 <p class="notice"><%= notice %></p>
